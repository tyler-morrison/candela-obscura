{
  "name": "candela-obscura",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/candela-obscura",
  "projectType": "application",
  "tags": [],
  "targets": {
    "build": {
      "command": "remix build",
      "options": {
        "cwd": "apps/candela-obscura"
      }
    },
    "serve": {
      "command": "remix dev",
      "options": {
        "cwd": "apps/candela-obscura"
      }
    },
    "start": {
      "dependsOn": ["build"],
      "command": "remix-serve build",
      "options": {
        "cwd": "apps/candela-obscura"
      }
    },
    "typecheck": {
      "command": "tsc",
      "options": {
        "cwd": "apps/candela-obscura"
      }
    }
  }
}
